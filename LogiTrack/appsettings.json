{
  "https_port": 443,
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },  
  "AllowedHosts": "localhost,*.logitrack.com",
 "ConnectionStrings": {
  "DefaultConnection": "Server=localhost;Port=3307;Database=logitrack;Uid=root;Pwd=123456;AllowPublicKeyRetrieval=true;SslMode=none;"
},
  "Jwt": {
    "Issuer": "LogiTrack",
    "Audience": "LogiTrackUsers",
    "Key": "ReplaceThisWithAStrongSecretKeyForDevOnly_ChangeInProd_MinimumLength32",
    "ExpiresMinutes": 60
  },
  "Security": {
    "PasswordExpirationDays": 90,
    "MaxFailedLoginAttempts": 5,
    "LockoutDurationMinutes": 15,
    "RequireEmailConfirmation": true
  }
}