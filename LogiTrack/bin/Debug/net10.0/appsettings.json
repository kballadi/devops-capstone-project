{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "localhost,*.logitrack.com",
  "Jwt": {
    "Issuer": "LogiTrack",
    "Audience": "LogiTrackUsers",
    "Key": "ReplaceThisWithAStrongSecretKeyForDevOnly_ChangeInProd_MinimumLength32",
    "ExpiresMinutes": 60
  },
  "Security": {
    "PasswordExpirationDays": 90,
    "MaxFailedLoginAttempts": 5,
    "LockoutDurationMinutes": 15,
    "RequireEmailConfirmation": true
  }
}